    stride: 2
  }
}
layer {
  name: "convL3_baseline"
  type: "Convolution"
  bottom: "poolL2_baseline"
  top: "convL3_baseline"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 128
    kernel_size: 3
