name: "mix"

layer {
    name: "data"
    type: "Python"
    top: "depth"
    top: "dpt3D"
    top: "joint"
    top: "clip_markers"
    top: "com"
    top: "config"
    top: "inds"
    python_param {
        module: "data_input_layer"
        layer: "NYUTrainSeq"
        param_str: '{"buffer_size": "8", "frame_size": "16", "augment": "false", "shuffle": "true", "size": "128"}'
    }
    include: {
        phase: TRAIN
    }
}
layer {
    name: "data"
    type: "Python"
    top: "depth"
    top: "dpt3D"
    top: "joint"
    top: "clip_markers"
    top: "com"
    top: "config"
    top: "inds"
    python_param {
        module: "data_input_layer"
        layer: "NYUTestSeq"
        param_str: '{"buffer_size": "1", "frame_size": "16", "augment": "false", "shuffle": "false", "size": "128"}'
    }
    include: {
        phase: TEST
    }
}
# lstm branch
layer {
