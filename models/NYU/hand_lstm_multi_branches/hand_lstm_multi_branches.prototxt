        }
    }
}

layer {
    name: "reshape_joint_C"
    type: "Reshape"
    bottom: "slice_joint_C"
    top: "reshape_joint_C"
    reshape_param {
        shape {
            dim: -1
            dim: 3
        }
    }
}

layer {
    name: "reshape_cm"
    type: "Reshape"
    bottom: "clip_markers"
    top: "reshape_cm"
    reshape_param {
        shape {
            dim: 16
            dim: 32
        }
    }
    include: {
        phase: TRAIN
    }
}

layer {
    name: "reshape_cm"
    type: "Reshape"
    bottom: "clip_markers"
    top: "reshape_cm"
    reshape_param {
        shape {
            dim: 16
            dim: 1
        }
    }
    include: {
        phase: TEST
    }
}

layer {
    name: "lstm_center"
    type: "LSTM"
    bottom: "reshape_data_center"
    bottom: "reshape_cm"
    top: "lstm_center"
    param {
    	lr_mult: 10
    }
    recurrent_param {
        num_output: 1024
        weight_filler {
            type: "uniform"
            min: -0.01
            max: 0.01
        }
        bias_filler {
            type: "constant"
            value: 0
        }
    }
}

layer {
    name: "lstm_drop_center"
    type: "Dropout"
    bottom: "lstm_center"
    top: "lstm_center"
    dropout_param {
        dropout_ratio: 0.5
    }
}

layer {
    name: "lstm_second"
    type: "LSTM"
    bottom: "reshape_data_second"
    bottom: "reshape_cm"
    top: "lstm_second"
    param {
    	lr_mult: 10
    }
    recurrent_param {
        num_output: 1024
        weight_filler {
            type: "uniform"
            min: -0.01
            max: 0.01
        }
        bias_filler {
            type: "constant"
            value: 0
        }
    }
}

layer {
    name: "lstm_drop_second"
    type: "Dropout"
    bottom: "lstm_second"
    top: "lstm_second"
    dropout_param {
        dropout_ratio: 0.5
    }
}

layer {
    name: "lstm_third"
    type: "LSTM"
    bottom: "reshape_data_third"
    bottom: "reshape_cm"
    top: "lstm_third"
    param {
    	lr_mult: 10
    }
    recurrent_param {
        num_output: 1024
        weight_filler {
