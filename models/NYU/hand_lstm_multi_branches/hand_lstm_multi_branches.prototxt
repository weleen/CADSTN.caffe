	bottom: "pred_T3"
	bottom: "reshape_joint_T3"
	top: "loss_T3"
	loss_weight: 1
}

layer {
	name: "loss_W1"
	type: "EuclideanLoss"
	bottom: "pred_W1"
	bottom: "reshape_joint_W1"
	top: "loss_W1"
	loss_weight: 1
}

layer {
	name: "loss_W2"
	type: "EuclideanLoss"
	bottom: "pred_W2"
	bottom: "reshape_joint_W2"
	top: "loss_W2"
	loss_weight: 1
}

layer {
	name: "total_loss"
    type: "Eltwise"
    bottom: "loss_P1"
    bottom: "loss_P2"
    bottom: "loss_R1"
    bottom: "loss_R2"
    bottom: "loss_M1"
    bottom: "loss_M2"
    bottom: "loss_I1"
    bottom: "loss_I2"
    bottom: "loss_T1"
    bottom: "loss_T2"
    bottom: "loss_T3"
    bottom: "loss_W1"
    bottom: "loss_W2"
    bottom: "loss_C"
    top: "total_loss"
    loss_weight: 0
}

layer {
	name: "pred_joint"
    type: "Concat"
    bottom: "pred_P1"
    bottom: "pred_P2"
    bottom: "pred_R1"
    bottom: "pred_R2"
    bottom: "pred_M1"
    bottom: "pred_M2"
    bottom: "pred_I1"
    bottom: "pred_I2"
    bottom: "pred_T1"
    bottom: "pred_T2"
    bottom: "pred_T3"
    bottom: "pred_W1"
    bottom: "pred_W2"
    bottom: "pred_C"
    top: "pred_joint"
    concat_param {
    	axis: 1
    }
    include: {
    	phase: TEST
    }
}

layer {
	name: "ignore"
	type: "Silence"
	bottom: "inds"
	bottom: "com"
	bottom: "config"
	bottom: "clip_markers"
	bottom: "gtorig"
    bottom: "gtcrop"
    bottom: "T"
    bottom: "gt3Dorig"
    bottom: "gt3Dcrop"
}

layer {
	name: "ignore"
	type: "Silence"
	bottom: "inds"
	bottom: "com"
	bottom: "config"
	bottom: "clip_markers"
	bottom: "gtorig"
    bottom: "gtcrop"
    bottom: "T"
    bottom: "gt3Dorig"
    bottom: "gt3Dcrop"
    bottom: "pred_joint"
    include: {
    	phase: TEST
    }
}