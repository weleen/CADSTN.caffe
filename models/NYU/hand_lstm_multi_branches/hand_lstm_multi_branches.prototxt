name: "hand_lstm_multi_branches"

layer {
    name: "data"
    type: "Python"
    top: "depth"
    top: "gtorig"
    top: "gtcrop"
    top: "T"
    top: "gt3Dorig"
    top: "gt3Dcrop"
    top: "joint"
    top: "clip_markers"
    top: "com"
    top: "config"
    top: "inds"
    python_param {
        module: "data_input_layer"
        layer: "NYUTrainSeq"
        param_str: '{"buffer_size": "32", "frame_size": "16", "shuffle": "true", "size": "128"}'
    }
    include: {
        phase: TRAIN
    }
}

layer {
    name: "data"
    type: "Python"
    top: "depth"
    top: "gtorig"
    top: "gtcrop"
    top: "T"
    top: "gt3Dorig"
    top: "gt3Dcrop"
    top: "joint"
    top: "clip_markers"
    top: "com"
    top: "config"
    top: "inds"
    python_param {
        module: "data_input_layer"
        layer: "NYUTestSeq"
        param_str: '{"buffer_size": "1", "frame_size": "16", "shuffle": "true", "size": "128"}'
    }
    include: {
        phase: TEST
    }
}

layer {
  name: "convL1"
  type: "Convolution"
  bottom: "depth"
  top: "convL1"
  param {
  	lr_mult: 0
    decay_mult: 0
  }
  param {
  	lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 8
    kernel_size: 5
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "ReLUconvL1"
  type: "ReLU"
  bottom: "convL1"
  top: "convL1"
}


layer {
  name: "poolL1"
  type: "Pooling"
  bottom: "convL1"
  top: "poolL1"
  pooling_param {
    pool: MAX
    kernel_size: 4
    stride: 4
  }
}

layer {
  name: "convL2"
  type: "Convolution"
  bottom: "poolL1"
  top: "convL2"
  param {
  	lr_mult: 0
    decay_mult: 0
  }
  param {
  	lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 8
    kernel_size: 5
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "ReLUconvL2"
  type: "ReLU"
  bottom: "convL2"
  top: "convL2"
}

layer {
  name: "poolL2"
  type: "Pooling"
  bottom: "convL2"
  top: "poolL2"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}


layer {
  name: "convL3"
  type: "Convolution"
  bottom: "poolL2"
  top: "convL3"
  param {
  	lr_mult: 0
    decay_mult: 0
  }
  param {
  	lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 8
    kernel_size: 3
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "ReLUconvL3"
  type: "ReLU"
  bottom: "convL3"
  top: "convL3"
}

layer {
  name: "FC1-center"
  type: "InnerProduct"
  bottom: "convL3"
  top: "FC1-center"
  param {
  	lr_mult: 1
    decay_mult: 1
  }
  param {
  	lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 1024
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "ReLUFC1-center"
  type: "ReLU"
  bottom: "FC1-center"
  top: "FC1-center"
}

layer {
  name: "DropoutFC1-center"
  type: "Dropout"
  bottom: "FC1-center"
  top: "FC1-center"
  dropout_param {
    dropout_ratio: 0.3
  }
}

layer {
  name: "FC1-second"
  type: "InnerProduct"
  bottom: "convL3"
  top: "FC1-second"
  param {
  	lr_mult: 1
    decay_mult: 1
  }
  param {
  	lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 1024
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "ReLUFC1-second"
  type: "ReLU"
  bottom: "FC1-second"
  top: "FC1-second"
}

layer {
  name: "DropoutFC1-second"
  type: "Dropout"
  bottom: "FC1-second"
  top: "FC1-second"
  dropout_param {
    dropout_ratio: 0.3
  }
}

layer {
  name: "FC1-third"
  type: "InnerProduct"
  bottom: "convL3"
  top: "FC1-third"
  param {
  	lr_mult: 1
    decay_mult: 1
  }
  param {
  	lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 1024
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}

layer {
  name: "ReLUFC1-third"
  type: "ReLU"
  bottom: "FC1-third"
  top: "FC1-third"
}

layer {
  name: "DropoutFC1-third"
  type: "Dropout"
  bottom: "FC1-third"
  top: "FC1-third"
  dropout_param {
    dropout_ratio: 0.3
  }
}

layer {
    name: "reshape_data_center"
    type: "Reshape"
    bottom: "FC1-center"
    top: "reshape_data_center"
    reshape_param {
        shape {
            dim: 16
            dim: 32
            dim: 1024
        }
    }
    include: {
        phase: TRAIN
    }
}

layer {
    name: "reshape_data_center"
    type: "Reshape"
    bottom: "FC1-center"
    top: "reshape_data_center"
    reshape_param {
        shape {
            dim: 16
            dim: 1
            dim: 1024
        }
    }
    include: {
        phase: TEST
    }
}

layer {
    name: "reshape_data_second"
    type: "Reshape"
    bottom: "FC1-second"
    top: "reshape_data_second"
    reshape_param {
        shape {
            dim: 16
            dim: 32
            dim: 1024
        }
    }
    include: {
        phase: TRAIN
    }
}

layer {
    name: "reshape_data_second"
    type: "Reshape"
    bottom: "FC1-second"
    top: "reshape_data_second"
    reshape_param {
        shape {
            dim: 16
            dim: 1
            dim: 1024
        }
    }
    include: {
        phase: TEST
    }
}

layer {
    name: "reshape_data_third"
    type: "Reshape"
    bottom: "FC1-third"
    top: "reshape_data_third"
    reshape_param {
        shape {
            dim: 16
            dim: 32
            dim: 1024
        }
    }
    include: {
        phase: TRAIN
    }
}

layer {
    name: "reshape_data_third"
    type: "Reshape"
    bottom: "FC1-third"
    top: "reshape_data_third"
    reshape_param {
        shape {
            dim: 16
            dim: 1
            dim: 1024
        }
    }
    include: {
        phase: TEST
    }
}

layer {
	name: "slice_joint"
	type: "Slice"
	bottom: "joint"
	top: "slice_joint_P1"
	top: "slice_joint_P2"
	top: "slice_joint_R1"
	top: "slice_joint_R2"
	top: "slice_joint_M1"
	top: "slice_joint_M2"
	top: "slice_joint_I1"
	top: "slice_joint_I2"
	top: "slice_joint_T1"
	top: "slice_joint_T2"
	top: "slice_joint_T3"
	top: "slice_joint_W1"
	top: "slice_joint_W2"
	top: "slice_joint_C"
	slice_param {
		axis: 1
		slice_point: 1
        slice_point: 2
        slice_point: 3
        slice_point: 4
        slice_point: 5
        slice_point: 6
        slice_point: 7
        slice_point: 8
        slice_point: 9
        slice_point: 10
        slice_point: 11
        slice_point: 12
        slice_point: 13
	}
}

layer {
    name: "reshape_joint_P1"
    type: "Reshape"
    bottom: "slice_joint_P1"
    top: "reshape_joint_P1"
    reshape_param {
        shape {
            dim: -1
            dim: 3
        }
    }
}

layer {
    name: "reshape_joint_P2"
    type: "Reshape"
    bottom: "slice_joint_P2"
    top: "reshape_joint_P2"
    reshape_param {
        shape {
            dim: -1
            dim: 3
        }
    }
}

layer {
    name: "reshape_joint_R1"
    type: "Reshape"
    bottom: "slice_joint_R1"
    top: "reshape_joint_R1"
    reshape_param {
        shape {
            dim: -1
            dim: 3
        }
    }
}

layer {
    name: "reshape_joint_R2"
    type: "Reshape"
    bottom: "slice_joint_R2"
    top: "reshape_joint_R2"
    reshape_param {
        shape {
            dim: -1
            dim: 3
        }
    }
}

layer {
    name: "reshape_joint_M1"
    type: "Reshape"
    bottom: "slice_joint_M1"
    top: "reshape_joint_M1"
    reshape_param {
        shape {
            dim: -1
            dim: 3
        }
    }
}

layer {
    name: "reshape_joint_M2"
    type: "Reshape"
    bottom: "slice_joint_M2"
    top: "reshape_joint_M2"
    reshape_param {
        shape {
            dim: -1
            dim: 3
        }
    }
}

layer {
    name: "reshape_joint_I1"
