	bottom: "pred_I2"
	bottom: "pred_I1_res"
	top: "pred_I1"
}

layer {
	name: "pred_T1"
	type: "Eltwise"
	bottom: "pred_T2"
	bottom: "pred_T1_res"
	top: "pred_T1"
}

# loss

layer {
	name: "loss_C"
	type: "EuclideanLoss"
	bottom: "pred_C"
	bottom: "reshape_joint_C"
	top: "loss_C"
	loss_weight: 1
}

layer {
	name: "loss_P1"
	type: "EuclideanLoss"
	bottom: "pred_P1"
	bottom: "reshape_joint_P1"
	top: "loss_P1"
	loss_weight: 1
}

layer {
	name: "loss_P2"
	type: "EuclideanLoss"
	bottom: "pred_P2"
	bottom: "reshape_joint_P2"
	top: "loss_P2"
	loss_weight: 1
