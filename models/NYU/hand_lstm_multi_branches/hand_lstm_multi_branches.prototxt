        }
    }
}

layer {
    name: "lstm_drop_third"
    type: "Dropout"
    bottom: "lstm_third"
    top: "lstm_third"
    dropout_param {
        dropout_ratio: 0.5
    }
}

layer {
	name: "pred_joint_center"
	type: "InnerProduct"
    bottom: "lstm_center"
    top: "pred_joint_center"
    param {
        lr_mult: 1
        decay_mult: 1
    }
    param {
        lr_mult: 2
        decay_mult: 0
    }
    inner_product_param {
        num_output: 3
        weight_filler {
            type: "xavier"
        }
        bias_filler {
            type: "constant"
        }
        axis: 2
    }
}

layer {
	name: "pred_joint_second"
	type: "InnerProduct"
    bottom: "lstm_second"
    top: "pred_joint_second"
    param {
        lr_mult: 1
        decay_mult: 1
    }
    param {
        lr_mult: 2
        decay_mult: 0
    }
    inner_product_param {
        num_output: 24 # 3 * 8
        weight_filler {
            type: "xavier"
        }
        bias_filler {
            type: "constant"
        }
        axis: 2
    }
}

layer {
	name: "pred_joint_third"
	type: "InnerProduct"
    bottom: "lstm_third"
    top: "pred_joint_third"
    param {
        lr_mult: 1
        decay_mult: 1
    }
    param {
        lr_mult: 2
        decay_mult: 0
    }
    inner_product_param {
        num_output: 15 # 3 * 5
        weight_filler {
            type: "xavier"
        }
        bias_filler {
            type: "constant"
        }
        axis: 2
    }
}

layer {
	name: "reshape_pred_joint_center"
	type: "Reshape"
	bottom: "pred_joint_center"
	top: "pred_C"
	reshape_param {
		shape {
			dim: -1
			dim: 3
		}
	}
}

layer {
	name: "reshape_pred_joint_second"
	type: "Reshape"
	bottom: "pred_joint_second"
	top: "reshape_pred_joint_second"
	reshape_param {
		shape {
			dim: -1
			dim: 24
		}
	}
}

layer {
	name: "slice_second"
	type: "Slice"
	bottom: "reshape_pred_joint_second"
	top: "pred_P2_res"
	top: "pred_R2_res"
	top: "pred_M2_res"
	top: "pred_I2_res"
	top: "pred_T3_res"
	top: "pred_T2_res"
	top: "pred_W1_res"
	top: "pred_W2_res"
	slice_param {
		axis: 1
		slice_point: 3
		slice_point: 6
		slice_point: 9
		slice_point: 12
		slice_point: 15
		slice_point: 18
		slice_point: 21
	}
}

layer {
	name: "pred_P2"
	type: "Eltwise"
	bottom: "pred_C"
	bottom: "pred_P2_res"
	top: "pred_P2"
}

layer {
	name: "pred_R2"
	type: "Eltwise"
	bottom: "pred_C"
	bottom: "pred_R2_res"
	top: "pred_R2"
}

layer {
	name: "pred_M2"
	type: "Eltwise"
	bottom: "pred_C"
	bottom: "pred_M2_res"
	top: "pred_M2"
}

layer {
	name: "pred_I2"
	type: "Eltwise"
	bottom: "pred_C"
	bottom: "pred_I2_res"
	top: "pred_I2"
}

layer {
	name: "pred_T3"
	type: "Eltwise"
	bottom: "pred_C"
	bottom: "pred_T3_res"
	top: "pred_T3"
}

layer {
	name: "pred_T2"
	type: "Eltwise"
	bottom: "pred_C"
	bottom: "pred_T2_res"
	top: "pred_T2"
}

layer {
	name: "pred_W1"
	type: "Eltwise"
	bottom: "pred_C"
	bottom: "pred_W1_res"
	top: "pred_W1"
}

layer {
	name: "pred_W2"
	type: "Eltwise"
	bottom: "pred_C"
	bottom: "pred_W2_res"
	top: "pred_W2"
}

layer {
	name: "reshape_pred_joint_third"
	type: "Reshape"
	bottom: "pred_joint_third"
	top: "reshape_pred_joint_third"
	reshape_param {
		shape {
			dim: -1
			dim: 15
		}
	}
}

layer {
	name: "slice_third"
	type: "Slice"
	bottom: "reshape_pred_joint_third"
	top: "pred_P1_res"
	top: "pred_R1_res"
	top: "pred_M1_res"
	top: "pred_I1_res"
	top: "pred_T1_res"
	slice_param {
		axis: 1
		slice_point: 3
		slice_point: 6
		slice_point: 9
		slice_point: 12
	}
}

layer {
	name: "pred_P1"
	type: "Eltwise"
	bottom: "pred_P2"
	bottom: "pred_P1_res"
	top: "pred_P1"
}

layer {
	name: "pred_R1"
	type: "Eltwise"
	bottom: "pred_R2"
	bottom: "pred_R1_res"
	top: "pred_R1"
}

layer {
	name: "pred_M1"
	type: "Eltwise"
	bottom: "pred_M2"
	bottom: "pred_M1_res"
	top: "pred_M1"
}

layer {
	name: "pred_I1"
