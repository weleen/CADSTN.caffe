layer {
	name: "fc1_center"
	type: "InnerProduct"
	bottom: "pool3"
	top: "fc1_center"
	param {
		lr_mult: 0
		decay_mult: 0
	}
	param {
		lr_mult: 0
		decay_mult: 0
	}
	inner_product_param {
		num_output: 2048
		weight_filler {
			type: "xavier"
		}
		bias_filler {
			type: "constant"
		}
	}
}

layer {
	name: "drop1_center"
    type: "Dropout"
    bottom: "fc1_center"
    top: "fc1_center"
    dropout_param {
    	dropout_ratio: 0.5
    }
}

layer {
	name: "fc1_second"
	type: "InnerProduct"
	bottom: "pool3"
	top: "fc1_second"
	param {
		lr_mult: 0
		decay_mult: 0
	}
	param {
		lr_mult: 0
		decay_mutl: 0
	}
	inner_product_param {
		num_output: 2048
		weight_filler {
			type: "xavier"
		}
		bias_filler {
			type: "constant"
		}
	}
}

layer {
