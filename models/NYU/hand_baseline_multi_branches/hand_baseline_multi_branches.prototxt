	name: "slice_third"
	type: "Slice"
	bottom: "pred_joint_third"
	top: "pred_P1"
	top: "pred_R1"
	top: "pred_M1"
	top: "pred_I1"
	top: "pred_T1"
	slice_param {
		axis: 1
		slice_point: 3
		slice_point: 6
		slice_point: 9
		slice_point: 12
	}
}

layer {
	name: "slice_joint"
	type: "Slice"
	bottom: "joint"
	top: "slice_joint_P1"
	top: "slice_joint_P2"
	top: "slice_joint_R1"
	top: "slice_joint_R2"
	top: "slice_joint_M1"
	top: "slice_joint_M2"
	top: "slice_joint_I1"
	top: "slice_joint_I2"
	top: "slice_joint_T1"
	top: "slice_joint_T2"
	top: "slice_joint_T3"
	top: "slice_joint_W1"
	top: "slice_joint_W2"
	top: "slice_joint_C"
	slice_param {
		axis: 1
		slice_point: 1
        slice_point: 2
        slice_point: 3
        slice_point: 4
        slice_point: 5
        slice_point: 6
        slice_point: 7
        slice_point: 8
        slice_point: 9
        slice_point: 10
        slice_point: 11
        slice_point: 12
        slice_point: 13
	}
}

layer {
	name: "loss_C"
	type: "EuclideanLoss"
	bottom: "pred_joint_center"
	bottom: "slice_joint_C"
	top: "loss_C"
	loss_weight: 1
}

layer {
	name: "loss_P1"
	type: "EuclideanLoss"
	bottom: "pred_P1"
	bottom: "slice_joint_P1"
	top: "loss_P1"
	loss_weight: 1
}

layer {
	name: "loss_P2"
	type: "EuclideanLoss"
	bottom: "pred_P2"
	bottom: "slice_joint_P2"
	top: "loss_P2"
	loss_weight: 1
}

layer {
	name: "loss_R1"
	type: "EuclideanLoss"
	bottom: "pred_R1"
	bottom: "slice_joint_R1"
