layer {
	name: "pred_joint_center"
	type: "InnerProduct"
    bottom: "fc2_center"
    top: "pred_joint_center"
    param {
        lr_mult: 1
        decay_mult: 1
    }
    param {
        lr_mult: 2
        decay_mult: 0
    }
    inner_product_param {
        num_output: 3
        weight_filler {
            type: "xavier"
        }
        bias_filler {
            type: "constant"
        }
        axis: 2
    }
    include: {
        phase: TRAIN
    }
}

layer {
	name: "pred_joint_center"
	type: "InnerProduct"
    bottom: "fc2_center"
    top: "pred_joint_center"
    param {
